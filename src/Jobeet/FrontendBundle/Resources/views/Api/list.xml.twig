<?xml version="1.0" encoding="utf-8"?>
<jobs>
	{% for url, job in jobs %}
		<job url="{{ url }}">
			{% for key, value in job %}
				<{{ key }}>{{ ( key == 'logo' and value ? app.request.host ~ asset('bundles/frontend/uploads/jobs/' ~ value) : value) }}</{{ key }}>
			{% endfor %}
		</job>
	{% endfor %}
</jobs>